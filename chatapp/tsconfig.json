{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "models/user.js", "libs/mongodb.js", "components/UserInfo.js", "components/LoginForm.js", "components/RegisterForm.js", "app/provider.js", "app/page.js", "app/layout.js", "app/register/page.js", "app/api/register/route.js", "app/api/auth/[...nextauth]/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
